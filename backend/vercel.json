{
    "version": 2,
    "name": "native-server",
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  },
    "routes":
     [{ "src": "/(.*)", "dest": "/index.js" }],
    "functions": {
      "api/**.js": {
        "runtime": "nodejs16.x"
      }
    }
  }
